<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../head') %>
  <% const title = 'Add New Game - OldSchoolGames' %>
  <% const description = 'Add a new classic MS-DOS game to the OldSchoolGames collection. Admin interface for managing retro games database.' %>
  <title><%= title %></title>
  <meta name="description" content="<%= description %>" />
  <meta property="og:title" content="<%= title %>" />
  <meta property="og:description" content="<%= description %>" />
  <meta property="og:url" content="https://oldschoolgames.eu/games/new" />
  <meta property="og:type" content="website" />
  <meta property="og:site_name" content="OldSchoolGames" />
  <meta property="og:image" content="https://oldschoolgames.eu/images/navbar.webp" />
</head>

<body class="layout">
  <%- include('../navbar') %>
  <%- include('../left-sidebar') %>
  <main>
    <div class="container">
      <h1>New Game</h1>
      <%- include('game-form') %>
    </div>
  </main>
  <%- include('../right-sidebar') %>
  <%- include('../footer') %>
</body>

</html>
